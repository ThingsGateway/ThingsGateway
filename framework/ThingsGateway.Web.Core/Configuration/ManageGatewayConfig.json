    "Password": "111111", //管理服务端口
    "DBDownTopic": "DBDownTopic", //下发网关配置的主题
    "DBUploadTopic": "DBUploadTopic", //子网关上传配置信息的主题
    "WriteRpcTopic": "WriteRpcTopic"
  }

